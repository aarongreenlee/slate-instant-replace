{
  "name": "slate-instant-replace",
  "version": "0.1.9",
  "description": "A Slate plugin that gives you full power on the last word your user typed.",
  "repository": "git://github.com/enzoferey/slate-instant-replace.git",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "*.md"
  ],
  "author": "Enzo Ferey",
  "license": "MIT",
  "peerDependencies": {
    "slate": "^0.44.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "immutable": "^3.8.2",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "slate": "^0.44.6",
    "slate-plain-serializer": "^0.6.30"
  },
  "scripts": {
    "babel": "babel ./lib/index.js -o ./dist/index.js",
    "prepublishOnly": "npm run prettier && npm run babel",
    "test": "mocha --require babel-core/register",
    "test:watch": "mocha --watch --require babel-core/register",
    "prettier": "prettier --config \"./.prettierrc.js\" --write \"./lib/**/*.@(js|scss)\""
  },
  "keywords": [
    "slate",
    "autocomplete",
    "replace",
    "editor",
    "last",
    "word"
  ],
  "dependencies": {}
}
