{
  "name": "slate-instant-replace",
  "version": "0.1.6",
  "description":
    "A Slate plugin to automatically replace text automatically when the user types certain strings.",
  "repository": "git://github.com/enzoferey/slate-instant-replace.git",
  "main": "./dist/index.js",
  "files": ["dist", "*.md"],
  "author": "Enzo Ferey",
  "license": "MIT",
  "peerDependencies": {
    "immutable": "^3.8.2",
    "slate": "^0.33.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "scripts": {
    "babel": "babel ./lib/index.js -o ./dist/index.js",
